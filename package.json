{
  "name": "liamnewmarch.co.uk",
  "description": "Liam Newmarch",
  "version": "8.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "node --experimental-modules --no-warnings src/build.js",
    "build:watch": "nodemon src/build.js",
    "deploy": "npm run build; gcloud app deploy --version=live",
    "serve": "http-server build -p 3000",
    "start": "concurrently npm:build:watch npm:serve"
  },
  "dependencies": {
    "concurrently": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.11.0",
    "front-matter": "^3.0.2",
    "fs-extra": "^8.0.1",
    "html-minifier": "^4.0.0",
    "http-server": "^0.11.1",
    "klaw": "^3.0.0",
    "marked": "^0.6.2",
    "nodemon": "^1.19.1",
    "nunjucks": "^3.2.0",
    "yaml": "^1.6.0"
  }
}
