{
  "private": true,
  "type": "module",
  "scripts": {
    "base": "esbuild ./src/static --bundle --entry-names=bundle",
    "analyse": "npm run build -- --analyze=verbose",
    "build": "npm run base -- --minify --outdir=src/static",
    "start": "npm run base -- --serve=3000 --servedir=src/static --sourcemap",
    "test": "eslint src/static --ignore-pattern=src/static/bundle.js"
  },
  "dependencies": {
    "esbuild": "^0.14.38",
    "stackdriver-errors-js": "^0.12.0"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0"
  }
}
